@Article{1995-Onuki-Fukuda,
	Author = "Akira Onuki and Jun-ichi Fukuda",
	Title = "{Electric field effects and form birefringence in diblock copolymers}",
	Journal = "Macromolecules",
	Volume = "28",
	Pages = "8788–8795",
	abstract = "We investigate effects of the electric field on diblock copolymers by assuming an induced dipolar interaction among the composition fluctuations. First, we show that, when a n electric field is applied perpendicularly to lamellae, undulations start to grow if their in-plane wavenumbers are smaller than an electric wavenumber qe proportional to the field. Subsequently, the undulations grow into larger spatial structures, eventually leading to a final square-lattice pattern. Second, we calculate the Maxwell stress tensor due to the electric field to predict a finite shear modulus in a lamellar state oriented by the electric field. Third, we examine form birefringence in disordered and ordered phases. In particular, the lamellar and hexagonal phases are shown to become birefringent on spatial scales longer than the spacing of lamellae or cylinders, even if the constituent monomers are optically isotropic. This gives rise to enhancement of depolarized light scattering from lamellar microstructures, which has indeed been observed recently. Most predictions in this paper are applicable to many situations other than those in the electric field.",
	doi = "10.1021/ma00130a011",
	keywords = "block copolymers; electric fields",
	localfile = {1995 - Electric field effects and form direfringence in diblock copolymers.pdf},
	year = 1995
}

@Article{1996-Kellogg-WMLRGS,
	Author = "G. J. Kellogg and D. G. Walton and Anne M. Mayes and P. Lambooy and Thomas P. Russell and P. D. Gallagher and S. K. Satija",
	Title = "{Observed surface energy effects in confined diblock copolymers}",
	Journal = "Phys. Rev. Lett.",
	Volume = "76",
	Number = "14",
	Pages = "2503–2506",
	abstract = "By using thin layers of random copolymer, we have varied the specificity of block-wall interactions for films of diblock copolymer confined between parallel walls. For highly specific interactions (bare walls), one block component segregates to both interfaces, and the frustration due to incommensuration between film thickness and the diblock’s bulk period is accommodated by the formation of layers parallel to the walls with perturbed period. In contrast, reduced specific interactions (random-copolymer-coated walls) allow the frustration to be accommodated by perpendicular ordering of the diblock, such that both block components are present at the walls.",
	doi = "10.1103/PhysRevLett.76.2503",
	keywords = "block copolymers; thin films",
	localfile = {1996 - Observed surface energy effects in confined diblock copolymers.pdf},
	rating = "3",
	year = 1996
}

