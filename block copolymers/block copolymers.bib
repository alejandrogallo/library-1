@Article{2004-DeRouchey-TR,
	Author = "J. DeRouchey and T. Thurn-Albrecht and Thomas P. Russell",
	Title = "{Block copolymer domain reorientation in an electric field: an in-situ small-angle X-ray scattering study}",
	Journal = "Macromolecules",
