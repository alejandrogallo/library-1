@Article{1980-Leibler,
	Author = "Ludwik Leibler",
	Title = "{Theory of microphase separation in block copolymers}",
	Journal = "Macromolecules",
	Volume = "13",
	Pages = "1602–1617",
	abstract = "A microscopic statistical theory of phase equilibria in noncrystalline block copolymer of type A-B is developed. In particular, the onset of an ordered mesophase from a homogeneous melt is studied and a criterion of the microphase separatin is found. The only relevant paarameters of hte theory turn out to be the product χN (χ is the Flory parameter characterizing A-B interactions; N is the polymerization index) and f, the fraction of monomers A in a chain. It is shown that under certain critical conditions a specific unstable mode appears in the homogeneous copolymer melt; this announces the microphase separation transition (MST). After the MST a mesophase with the periodicity equal to the wavelength of the unstable mode and with the symmetry of a body-centered-cubic (BCC) lattive should appear. For a large range of compositions a bcc mesophase is expected to be a metastable phase. Only two other ordered phases, a hexagonal mesophase and a lamellar mesophase, may be stable near the MST. The regions of stability are calculated and the phase diagram of the system is provided. First-order transition are predicted between different ordered phases. A new method of measuring the parameter χ, crucial for verification of the theory, is proposed.",
	doi = "10.1021/ma60078a047",
	localfile = {1980 - Theory of microphase separation in block copolymers.pdf},
	year = 1980
}

