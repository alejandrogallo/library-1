	keywords = "block copolymers; hydrodynamics; nanorods; nanospheres; nanotriangles; polymer blends; polymer nanocomposites; rheology",
	localfile = {2009 - Non-equilibrium particle-field simulations of polymer-nanocomposite dynamics.pdf},
