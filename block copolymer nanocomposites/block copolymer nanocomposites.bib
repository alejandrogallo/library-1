	doi = "10.1021/nl048407f",
	keywords = "block copolymers; mechanical properties; nanoparticle distribution; nanospheres; polymer nanocomposites",
	localfile = {2004 - Origins of elastic properties in ordered block copolymer-nanoparticle composites.pdf},
	methods = "SCFT",
	year = 2004
}

