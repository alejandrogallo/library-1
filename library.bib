@Article{2009-Cysewski,
	Author = "Piotr Cysewski",
	Title = "{Intra-strand stacking interactions in {B-DNA} crystals characterized by {post-SCF} quantum chemistry computations}",
	Journal = "New J. Chem.",
	Volume = "33",
	Number = "9",
	Pages = "1909–1917",
	abstract = "The distributions of intermolecular interaction energies {(IIE)} of all possible 5[prime or {minute]-X/Y-3[prime} or minute] {(X,} Y = A, G, T, C) stacked pairs in conformations exactly matching to intra-strand orientations occurring in crystallographic {B-DNA} double strands were characterized by quantum chemistry calculations on {MP2/aug-cc-pvDZ} level. The essential feature of this approach is sampling over meaningful conformations and getting statistically significant data on the energetics of stacking interactions in {B-DNA} crystals. Based on the most frequently occurring {IIE} values (in kcal mol-1) the following order of the intra-strand stacking interactions in {B-DNA} solids is concluded: {G/A} -10.2 +/- 1.1 {\textgreater} {A/G} -9.2 +/- 1.0 [approximate] {G/C} -8.7 +/- 1.2 {\textgreater} {A/A} -8.3 +/- 1.0 {\textgreater} {A/T} -7.7 +/- 0.9 {\textgreater} {C/G} -7.2 +/- 0.9 {\textgreater} {T/G} -6.5 +/- 1.3 [approximate] {A/C} -6.2 +/- 0.8 [approximate] {G/T} -6.2 +/- 1.3 {\textgreater} {T/A} -5.6 +/- 0.9 {\textgreater} {G/G} -5.1 +/- 1.1 {\textgreater} {T/C} -4.9 +/- 0.7 {\textgreater} {C/T} -4.8 +/- 1.1 {\textgreater} {T/T} -4.7 +/- 0.5 {\textgreater} {C/A} -3.8 +/- 0.8 {\textgreater} {C/C} -1.0 +/- 1.0 (standard deviations are provided in parenthesis). Sequence-related structural diversities and {SCF} and correlation contributions to the stacking interactions are also discussed. The most representative structures of stacked dimers were found based on a clustering protocol.",
	collection = "stacking",
	doi = "10.1039/b909327c",
	keywords = "aromatic stacking",
	localfile = "molecular stacking/2009 - Intra-strand stacking interactions in B-DNA crystals characterized by post-SCF quantum chemistry computations.pdf",
	methods = "MP2/aug-cc-pvDZ",
	year = 2009
}

@Article{2009-Czyznikowska,
	Author = "Żaneta Czyżnikowska",
	Title = "{On the importance of electrostatics in stabilization of stacked guanine–adenine complexes appearing in {B-DNA} crystals}",
