@Article{1993-Chaires-PGB,
	Author = "Jonathan B. Chaires and Waldemar Priebe and David E. Graves and T. G. Burke",
	Title = "{Dissection of the free energy of anthracycline antibiotic binding to {DNA:} electrostatic contributions}",
	Journal = "J. Am. Chem. Soc.",
	Volume = "115",
	Number = "13",
	Pages = "5360–5364",
	abstract = "Fluorescence titration methods and equilibrium dialysis were used to study the thermodynamics of the interaction of doxorubicin, the @ anomer of doxorubicin, daunorubicin, and hydroxyrubicin with DNA. All of these except hydroxyrubicin carry a net charge of +1 a t neutral pH, arising from the protonation of the daunosamine moiety. Hydroxyrubicin is a synthetic anthracycline antibiotic in which the amine moiety has been replaced by a hydroxyl group, whicfi is uncharged but polar at neutral pH. The comparative binding studies we describe offer a unique opportunity to evaluate the electrostatic contributions to the D N A binding free energy of these anthracycline antibiotics and to test specific predictions arising from current polyelectrolyte theory as applied to ligand-DNA interactions. We find that the quantity (6 log K/6 log [M+]) changes from a value of -0.95 for doxorubicin to -0.18 for hydroxyrubicin. The latter value is in excellent agreement with the value of -0.24 predicted by the theory of Friedman and Manning for the interaction of an uncharged intercalating ligand with DNA. The D N A binding free energy decreases from -8.8 kcal mol-' for doxorubicin to -7.2 kcal mol-' for hydroxyrubicin under solution conditions of 200 mM Na+, p H 7.0, 20 OC. The results we obtain allow us to dissect the D N A binding free energy into its electrostatic and nonelectrostatic components. Knowledge of these values helps to clarify the precise role of the amine group in doxorubicin binding to DNA. In addition, comparison of the D N A binding of doxorubicin with the /3 anomer of doxorubicin provides a striking example of stereoselective antibiotic binding to DNA.",
	collection = "nucleic acid intercalation",
	doi = "10.1021/ja00066a002",
	localfile = "nucleic acid intercalation/1992 - Dissection of the free energy of anthracycyline antibiotic binding to DNA: electrostatic contributions.pdf",
	shorttitle = "Dissection of the free energy of anthracycline antibiotic binding to {DNA}",
	year = 1993
}

