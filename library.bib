@Article{2006-Rafat-SP,
	Author = "Michel Rafat and Majeed Shaik and Paul L. A. Popelier",
	Title = "{Transferability of Quantum Topological Atoms in Terms of Electrostatic Interaction Energy}",
	Journal = "J. Phys. Chem. A",
