}

@Article{2009-Hill-Platts,
	Author = "J. Grant Hill and James A. Platts",
	Title = "{Local electron correlation descriptions of the intermolecular stacking interactiosn between aromatic intercalators and nucleic acids}",
	Journal = "Chem. Phys. Lett.",
