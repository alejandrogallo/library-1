	doi = "10.1016/j.mrfmmm.2007.03.006",
	localfile = "nucleic acid intercalation/2007 - Assessment of atypical DNA intercalating agents in biological and in silico systems.pdf",
	year = 2007
}

@Article{2007-Stephens-PB,
