@Article{2009-Day-CCHABTVVJGDTEFBFHNLKPMKWSASSWS,
	Author = "Graeme M. Day and Timothy G. Cooper and Aurora J. {Cruz-Cabeza} and Katarzyna E. Hejczyk and Herman L. Ammon and Stephan X. M. Boerrigter and Jeffrey S. Tan and Raffaele G. Della Valle and Elisabetta Venuti and Jovan Jose and Shridhar R. Gadre and Gautam R. Desiraju and Tejender S. Thakur and {Bouke P. van} Eijck and Julio C. Facelli and Victor E. Bazterra and Marta B. Ferraro and Detlef W. M. Hofmann and Marcus A. Neumann and Frank J. J. Leusen and John Kendrick and Sarah L. Price and Alston J. Misquitta and Panagiotis G. Karamertzanis and Gareth W. A. Welch and Harold A. Scheraga and Yelena A. Arnautova and Martin U. Schmidt and {Jacco van de} Streek and Alexandra K. Wolf and Bernd Schweizer",
	Title = "{Significant progress in predicting the crystal structures of small organic molecules – a report on the fourth blind test}",
	Journal = "Acta Cryst. B",
	Volume = "65",
	Number = "2",
	Pages = "107–125",
	abstract = "We report on the organization and outcome of the fourth blind test of crystal structure prediction, an international collaborative project organized to evaluate the present state in computational methods of predicting the crystal structuresof small organic molecules. There were 14 research groups which took part, using a variety of methods to generate and rank the most likely crystal structures for four target systems: three single-component crystal structures and a 1:1 cocrystal. Participants were challenged to predict the crystal structures of the four systems, given only their molecular diagrams, while the recently determined but as-yet unpublished crystal structures were withheld by an independent referee. Three predictions were allowed for each system. The results demonstrate a dramatic improvement in rates of success over previous blind tests; in total, there were 13 successful predictions and, for each of the four targets, at least two groups correctly predicted the observed crystal structure. The successes include one participating group who correctly predicted all four crystal structures as their ﬁrst ranked choice, albeit at a considerable computational expense. The results reﬂect important improvements in modelling methods and suggest that, at least for the small and fairly rigid types of molecules included in this blind test, such calculations can be constructively applied to help understand crystallization and polymorphism of organic molecules.",
	collection = "molecular crystals",
	doi = "10.1107/S0108768109004066",
	localfile = "molecular crystals/2009 - Significant progress in predicting the crystal structures of small organic molecules - a report on the fourth blind test.pdf",
	year = 2009
}

