@Article{1992-Sokalski-SRO,
	Author = "W. Andrzej Sokalski and M. Shibata and Rein Rein and Rick L. Ornstein",
	Title = "{Cumulative atomic multipole moments complement any atomic charge model to obtain more accurate electrostatic properties}",
