@Article{2002-Noguchi-Takasu,
	Author = "Hiroshi Noguchi and Masako Takasu",
	Title = "{Adhesion of nanoparticles to vesicles: a Brownian dynamics simulation}",
	Journal = "Biophys. J.",
	Volume = "83",
	Pages = "299–308",
	abstract = "We studied the interaction of bilayer vesicles and adhesive nanoparticles using a brownian dynamics simulation. The nanoparticles are simple models of proteins aor colloids. The adhering nanoparticle induces the morphological change of the vesicle: budding, formation of two vesicles in which only the outer monolayers are connected, and fission. We also show that the nanoparticle promotes the fusion process: fusion-pore opening from a stalk intermediate, a neck-like structure that only connects outer monolayers of two vesicles. The nanoparticle blends the stalk, and induces the pore opening.",
	doi = "10.1016/S0006-3495(02)75170-9",
	keywords = "biological nanocomposites; lipid bilayers; nanospheres",
	localfile = {2002 - Adhesion of nanoparticles to vesicles: a Brownian dynamics simulation.pdf},
	methods = "CG BD",
	year = 2002
}

