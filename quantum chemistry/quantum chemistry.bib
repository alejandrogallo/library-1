@Article{2006-Ishimura-PN,
	Author = "Kazuya Ishimura and Peter Pulay and Shigeru Nagase",
	Title = "{A new parallel algorithm of {MP2} energy calculations}",
	Journal = "J. Comp. Chem.",
	Volume = "27",
	Number = "4",
	Pages = "407–413",
	abstract = "A new parallel algorithm has been developed for second-order {Møller-Plesset} perturbation theory {(MP2)} energy calculations. Its main projected applications are for large molecules, for instance, for the calculation of dispersion interaction. Tests on a moderate number of processors (2-16) show that the program has high {CPU} and parallel efficiency. Timings are presented for two relatively large molecules, taxol {(C47H51NO14)} and luciferin {(C11H8N2O3S2),} the former with the {6-31G*} and {6-311G**} basis sets (1032 and 1484 basis functions, 164 correlated orbitals), and the latter with the {aug-cc-pVDZ} and {aug-cc-pVTZ} basis sets (530 and 1198 basis functions, 46 correlated orbitals). An {MP2} energy calculation on {C130H10} (1970 basis functions, 265 correlated orbitals) completed in less than 2 h on 128 processors.",
	doi = "10.1002/jcc.20348",
	keywords = "algorithms",
	localfile = {2006 - A new parallel algorithm of MP2 energy calculations.pdf},
	methods = "MP2",
	year = 2006
}

@Article{2007-Bader,
	Author = "Richard F. W. Bader",
	Title = "{Everyman's Derivation of the Theory of Atoms in Molecules}",
	Journal = "J. Phys. Chem. A",
	Volume = "111",
	Number = "32",
	Pages = "7966–7972",
	abstract = "This paper demonstrates that it is straightforward to develop the theory of an atom in a moleculesthe extension of quantum mechanics to an open systemsby deriving the necessary equations of motion from Schrodinger’s equation, followed by a comparison of the predicted properties with experiment to determine the correct boundary condition. Although less fundamental than the variational derivation of the quantum theory of atoms in molecules, this heuristic approach makes the quantum mechanics of an atom in a molecule accessible to “everyman” possessing a knowledge of Schrodinger’s equation, aiding its general acceptance by experimental chemists.",
	doi = "10.1021/jp073213k",
	localfile = {2007 - Everyman's derivation of the theory of atoms in molecules.pdf},
	year = 2007
}

