@Article{1964-Hohenberg-Kohn,
	Author = "P. Hohenberg and W. Kohn",
	Title = "{Inhomogeneous electron gas}",
	Journal = "Phys. Rev. B",
	Volume = "136",
	Number = "3B",
	Pages = "B864–B871",
	abstract = "This paper deals with the ground state of an interacting electorn gas in an external potential v(r). It is proved that there exists a universal functional of the density, F[n(r)], independent of v(r), such that the expression $E=\intv(r)n(r)dr + F[n(r)] has as its minimum value the correct ground-state energy associated with v(r). The functional F[n(r)] is the ndiscussed for two situationsL (1) $n(r) = n\_0 + \tilde{n}(r), \tilde{n}/n\_0 << 1$, and (2) $n(r) = \phi(r/r\_0)$ with $\phi$ arbitrary and $r\_0 \rightarrow \infty$. In both cases $F$ can be expressed entirely in terms of the correlation energy and linear and higher order electronic polarizabilities of a uniform electorn gas. This approach also sheds some light on generlized Thomas-Fermi methods and their limitations. Some new extensions of these methods are presented.$",
	doi = "10.1103/PhysRev.136.B864",
	localfile = {1964 - Inhomogeneous electron gas.pdf},
	year = 1964
}

@Article{1965-Kohn-Sham,
