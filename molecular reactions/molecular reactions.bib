@Article{2001-Martin-Feher,
	Author = "P. A. Martin and M. Feher",
	Title = "{Ab initio calculations on the multipole moments and dipole polarisabilities of the {PO+} molecular ion {(X1[Sigma]+)}}",
	Journal = "J. Mol. Struct.: {THEOCHEM}",
	Volume = "540",
	Number = "1-3",
	Pages = "139–144",
	abstract = "The potential energy, dipole, quadrupole and octopole moments and dipole polarisabilities have been calculated at {CASSCF} level for the ground {X1[Sigma]+} state of the {PO+} molecular ion as a function of internuclear distance. Most of the electrical properties have not previously been calculated and show rapid variations around 5 a.u. due to a perturbation. The calculated vibrational frequency of 1410.4 cm-1 and the integrated {IR} absorption intensity of 984 cm2 mol-1 should lead towards the first observation of the vibrational spectrum.",
	doi = "10.1016/S0166-1280(00)00727-2",
	localfile = {2001 - Ab initio calculations on the multipole moments and dipole polarisabilities of the PO+ molecular ion.pdf},
	year = 2001
}

