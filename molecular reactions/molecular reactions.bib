@Article{2005-Weck-Balakrishnan,
	Author = "P F Weck and N Balakrishnan",
	Title = "{Quantum dynamics of the Li + {HF} –> H + {LiF} reaction at ultralow temperatures}",
	Journal = "J. Chem. Phys.",
	Volume = "122",
	Number = "15",
	Pages = "154309",
	abstract = "Quantum-mechanical calculations are reported for the {Li+HF(v=0,1,j=0)->H+LiF(v',j')} bimolecular scattering process at low and ultralow temperatures. Calculations have been performed for zero total angular momentum using a recent high-accuracy potential-energy surface for the {X2A'} electronic ground state. For {Li+HF(v=0,j=0),} the reaction is dominated by resonances due to the decay of metastable states of the Li {cdots,...F-H} van der Waals complex. Assignment of these resonances has been carried out by calculating the eigenenergies of the quasibound states. We also find that while chemical reactivity is greatly enhanced by vibrational excitation, the resonances get mostly washed out in the reaction of vibrationally excited {HF} with Li atoms. In addition, we find that at low energies, the reaction is significantly suppressed due to the less-efficient tunneling of the relatively heavy fluorine atom.",
	doi = "10.1063/1.1884115",
	localfile = {2005 - Quantum dynamics of the Li+HF->H+LiF reaction at ultralow temperatures.pdf},
	year = 2005
}

